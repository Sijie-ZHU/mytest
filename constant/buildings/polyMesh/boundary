FoamFile
{
    version     2.0;
    format      ascii;
    class       polyBoundaryMesh;
    location    "constant/buildings/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

2
(
    buildings_to_concrete
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          64;
        startFace       2400;
        sampleMode      nearestPatchFace;
        sampleRegion    concrete;
        samplePatch     concrete_to_buildings;
    }
    buildings_to_air
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          512;
        startFace       2464;
        sampleMode      nearestPatchFace;
        sampleRegion    air;
        samplePatch     air_to_buildings;
    }
)

