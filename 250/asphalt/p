FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "250/asphalt";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 100000;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    side
    {
        type            calculated;
        value           uniform 0;
    }
    asphalt_to_soil1
    {
        type            calculated;
        value           uniform 0;
    }
    asphalt_to_air
    {
        type            calculated;
        value           uniform 0;
    }
    asphalt_to_concrete
    {
        type            calculated;
        value           uniform 0;
    }
    asphalt_to_soil2
    {
        type            calculated;
        value           uniform 0;
    }
}


