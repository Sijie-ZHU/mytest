FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 300;

boundaryField
{
    inlet
    {
        type            zeroGradient;
        value           uniform 300;
    }
    outlet
    {
        type            zeroGradient;
        value           uniform 300;
    }
    side
    {
       type            zeroGradient;
        value           uniform 300; 
    }
    soil2_to_soil3
    {
        type            fixedValue;
        value           uniform 300;
    }
    soil2_to_soil1
    {
       type            fixedValue;
       value           uniform 300;
    }
}


