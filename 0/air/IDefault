FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/air";
    object      IDefault;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 0 -3 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            wideBandDiffusiveRadiation;
        value           uniform 0;
    }
    outlet
    {
        type            wideBandDiffusiveRadiation;
        value           uniform 0;
    }
    side
    {
        type            wideBandDiffusiveRadiation;
        value           uniform 0;
    }
    top
    {
        type            wideBandDiffusiveRadiation;
        value           uniform 0;
    }
    air_to_soil3
    {
        type            wideBandDiffusiveRadiation;
        value           uniform 0;
    }
    air_to_buildings
    {
        type            wideBandDiffusiveRadiation;
        value           uniform 0;
    }
}


